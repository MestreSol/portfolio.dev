{
  "name": "portfolio.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && next dev --turbopack",
    "services:up": "docker compose -f infra/compose.yaml --env-file .env.development up -d",
    "services:stop": "docker compose -f infra/compose.yaml --env-file .env.development stop",
    "services:down": "docker compose -f infra/compose.yaml --env-file .env.development down",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "eslint-plugin-prettier": "^5.2.3",
    "next": "latest",
    "pg": "^8.14.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@next/eslint-plugin-next": "^15.2.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.5.3",
    "typescript": "^5"
  }
}
